database demo;

table data_table {
    ts int primary notnull,
    content text
}

action log_store_ingest ($content, $timestamp) public {
  INSERT INTO data_table (ts, content) VALUES ($timestamp, $content);
}

action get_data() public view {
    SELECT * FROM data_table;
}

action test() public view {
    // just output a string
    SELECT @caller as value;
}